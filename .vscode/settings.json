{
  "[docker,shell]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[js,json,md,yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "files.associations": {
    "*.gotmpl": "helm",
    "*.tmpl": "nginx",
    "*.yml": "yaml",
    "*.tpl": "yaml",
    "helmfile-*.yaml": "helm",
    "helmfile.yaml": "helm"
  }
}
