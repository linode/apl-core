{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "downlevelIteration": true,
    "plugins": [
      {
        "transform": "ts-auto-mock/transformer",
        "cacheBetweenTests": false
      }
    ],
    "target": "esnext"
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src", "jest.config.ts"],
  "types": ["jest"]
}
