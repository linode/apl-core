                  "items": {
                    "type": "string"
                  }
                },
                "secretName": {
                  "type": "string"
                }
              }
            }
          }
        }
      },
      "lifecycleHooks": {
        "type": "string"
      },
      "livenessProbe": {
        "type": "string"
