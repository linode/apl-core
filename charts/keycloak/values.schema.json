          "maxReplicas": {
            "type": "integer"
          },
          "metrics": {
            "type": "array"
          },
          "behavior": {
            "type": "object"
          }
        }
      },
      "test": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "boolean"
          },
          "image": {
            "$ref": "#/definitions/image"
          },
          "podSecurityContext": {
            "type": "object"
          },
          "securityContext": {
            "type": "object"
          }
        }
      },
      "tolerations": {
        "type": "array"
      }
    }
  }
}
