{{- $v := .Values }}

nameOverride: hello
fullnameOverride: hello

resources:
  requests:
    cpu: 10m
    memory: 8Mi
  limits:
    cpu: 100m
    memory: 64Mi

podSecurityContext:
  runAsUser: 1000

{{- with $v._files | get "otomi.globalPullSecret" nil }}
imagePullSecrets:
  - name: otomi-pullsecret-global
{{- end }}
