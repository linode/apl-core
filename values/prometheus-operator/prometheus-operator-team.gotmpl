defaultRules:
  create: false

kubeApiServer:
  enabled: false
kubelet:
  enabled: true
nodeExporter:
  enabled: false
prometheusOperator:
  enabled: false
  manageCrds: false
prometheus:
  resources: {}
  additionalServiceMonitors: null
  prometheusSpec:
    enableAdminAPI: false
additionalPrometheusRules: null
grafana:
  defaultDashboardsEnabled: false

alertmanager:
  resources:
    requests:
      memory: 128Mi
      cpu: 100m
    limits:
      memory: 256Mi
      cpu: 500m
