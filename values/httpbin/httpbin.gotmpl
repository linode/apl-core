{{- $v := .Values }}
{{- $httpbin := $v.charts | get "httpbin" dict }}

nameOverride: httpbin
fullnameOverride: httpbin

resources:
  requests:
    cpu: 10m
    memory: 32Mi
  limits:
    cpu: 50m
    memory: 64Mi
podAnnotations:
  policy.otomi.io/ignore: psp-allowed-users,banned-image-tags

{{- with .Values.otomi | get "globalPullSecret" nil }}
imagePullSecrets:
  - name: otomi-pullsecret-global
{{- end }}
