{{- $v := .Values }}
clusterName: {{ $v.cluster.apiName }}
autoDiscoverAwsRegion: true
autoDiscoverAwsVpcID: true
priorityClassName: otomi-critical

resources:
  requests:
    cpu: 500m
    memory: 64Mi
  limits:
    cpu: 1
    memory: 256Mi

{{- with $v._files | get "otomi.globalPullSecret" nil }}
image:
  pullSecrets:
    - otomi-pullsecret-global
{{- end }}
